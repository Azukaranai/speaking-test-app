{
  "name": "speaking-test-app",
  "private": true,
  "version": "0.1.21",
  "type": "commonjs",
  "scripts": {
    "predev": "node scripts/sync-data.js",
    "dev": "vite",
    "prebuild": "node scripts/sync-data.js",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && npx netlify deploy --dir=dist",
    "deploy:prod": "npm run build && npx netlify deploy --prod --dir=dist",
    "deploy:cf": "npm run build && npx wrangler pages deploy dist --project-name speaking-test-app",
    "tts:pick-voices": "node scripts/tts-pick-voices.js",
    "tts:generate": "node scripts/tts-generate.js",
    "tts:verify": "node scripts/tts-verify.js"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "microsoft-cognitiveservices-speech-sdk": "^1.37.0"
  },
  "devDependencies": {
    "vite": "^5.4.0"
  }
}
